{
  "name": "cambis/silverstan-basic-app",
  "require": {
    "php": "^8.1",
    "silverstripe/cms": "^5.3",
    "silverstripe/framework": "^5.3"
  },
  "require-dev": {
    "cambis/silverstan": "dev-main"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../",
      "version": "dev-main"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/src"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
